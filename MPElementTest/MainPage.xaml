        <Button x:Name="StepForwardOneFrame" Content="StepForwardOneFrame" Click="StepForwardOneFrame_Test_Click" Margin="154,858,0,0" VerticalAlignment="Top"/>
        <Button x:Name="test" Content="Test1" Click="Test1_Click" Margin="382,858,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="notify" Text="notify" Height="25" Margin="454,861,448,14"/>
        <Button x:Name="frequency" Content="frequency" Click="frequency_Click" Margin="1098,858,0,0" VerticalAlignment="Top"/>
        <Button x:Name="exit" Content="Exit" Click="exit_Click" Margin="1440,858,0,0" VerticalAlignment="Top"/>
    </Grid>
</Page>
